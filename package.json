{
  "name": "chatsumm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill:ports && npm run setup:backend && concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "dev:frontend": "next dev",
    "dev:backend": "python backend/run_server.py",
    "setup:backend": "node setup_backend.js",
    "kill:ports": "node kill_ports.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "next": "16.0.1"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "concurrently": "^9.1.0"
  }
}
